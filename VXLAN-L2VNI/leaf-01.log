﻿nv overlay evpn
feature ospf
feature bgp
feature pim
feature interface-vlan
feature vn-segment-vlan-based
feature lldp
feature nv overlay
!
hostname leaf-01
!
ip pim rp-address 10.12.12.12 group-list 224.0.0.0/4
ip pim ssm range 232.0.0.0/8
vlan 1,100
!
vlan 100
  vn-segment 100100
!
interface Vlan100
  no shutdown
  ip address 192.168.100.1/24
!
interface Ethernet1/1
  mtu 9216
  medium p2p
  ip unnumbered loopback0
  ip ospf network point-to-point
  ip router ospf 100 area 0.0.0.0
  ip pim sparse-mode
  no shutdown
!
interface Ethernet1/3
  mtu 9216
  medium p2p
  ip unnumbered loopback0
  ip ospf network point-to-point
  ip router ospf 100 area 0.0.0.0
  ip pim sparse-mode
  no shutdown
!
interface loopback0
  ip address 3.3.3.3/32
  ip ospf network point-to-point
  ip router ospf 100 area 0.0.0.0
  ip pim sparse-mode
!
interface loopback1
  ip address 10.3.3.3/32
  ip pim sparse-mode
!
router ospf 100
router bgp 65000
  router-id 3.3.3.3
  log-neighbor-changes
  address-family ipv4 unicast
    network 10.3.3.3/32
  address-family l2vpn evpn
  template peer SPINES
    remote-as 65000
    update-source loopback0
    address-family ipv4 unicast
      send-community extended
    address-family l2vpn evpn
      send-community extended
  neighbor 1.1.1.1
    inherit peer SPINES
  neighbor 2.2.2.2
    inherit peer SPINES
exit
exit
exit
!
copy run start


