﻿hostname spine-02
!
nv overlay evpn
feature ospf
feature bgp
feature pim
feature lldp
feature nv overlay
!
ip pim rp-address 10.12.12.12 group-list 224.0.0.0/4
ip pim ssm range 232.0.0.0/8
ip pim anycast-rp 10.12.12.12 1.1.1.1
ip pim anycast-rp 10.12.12.12 2.2.2.2
!
interface Ethernet1/1
  mtu 9216
  medium p2p
  ip unnumbered loopback0
  ip ospf network point-to-point
  ip router ospf 100 area 0.0.0.0
  ip pim sparse-mode
  no shutdown
!
interface Ethernet1/3
  mtu 9216
  medium p2p
  ip unnumbered loopback0
  ip ospf network point-to-point
  ip router ospf 100 area 0.0.0.0
  ip pim sparse-mode
  no shutdown
!
interface Ethernet1/5
  mtu 9216
  medium p2p
  ip unnumbered loopback0
  ip ospf network point-to-point
  ip router ospf 100 area 0.0.0.0
  ip pim sparse-mode
  no shutdown
!
interface Ethernet1/7
  mtu 9216
  medium p2p
  ip unnumbered loopback0
  ip ospf network point-to-point
  ip router ospf 100 area 0.0.0.0
  ip pim sparse-mode
  no shutdown
!
interface loopback0
  ip address 2.2.2.2/32
  ip ospf network point-to-point
  ip router ospf 100 area 0.0.0.0
  ip pim sparse-mode
!
interface loopback100
  ip address 10.12.12.12/32
  ip ospf network point-to-point
  ip router ospf 100 area 0.0.0.0
  ip pim sparse-mode
!
router ospf 100
router bgp 65000
  router-id 2.2.2.2
  log-neighbor-changes
  address-family ipv4 unicast
  address-family l2vpn evpn
  template peer VTEPS
    remote-as 65000
    update-source loopback0
    address-family ipv4 unicast
      send-community extended
      route-reflector-client
    address-family l2vpn evpn
      send-community extended
      route-reflector-client
  neighbor 3.3.3.3
    inherit peer VTEPS
  neighbor 4.4.4.4
    inherit peer VTEPS
  neighbor 5.5.5.5
    inherit peer VTEPS
  neighbor 6.6.6.6
    inherit peer VTEPS
end
copy run start


